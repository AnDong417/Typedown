<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:windows="clr-namespace:Typedown.Windows" >
    <Style x:Key="DefaultAppWindowStyle" TargetType="windows:AppWindow">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="windows:AppWindow">
                    <Border x:Name="PART_RootElement">
                        <ContentPresenter Content="{TemplateBinding Content}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>