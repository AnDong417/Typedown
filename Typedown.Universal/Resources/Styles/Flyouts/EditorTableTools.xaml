<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <MenuFlyout x:Key="EditorTableTools" Placement="RightEdgeAlignedTop">
        <MenuFlyoutItem x:Uid="InsertRowAbove" Name="InsertPreviousRow"/>
        <MenuFlyoutItem x:Uid="InsertRowBelow" Name="InsertNextRow"/>
        <MenuFlyoutItem x:Uid="RemoveRow" Name="RemoveNurrentRow"/>
        <MenuFlyoutItem x:Uid="InsertColumnLeft" Name="InsertLeftColumn"/>
        <MenuFlyoutItem x:Uid="InsertColumnRight" Name="InsertRightColumn"/>
        <MenuFlyoutItem x:Uid="RemoveColumn" Name="RemoveVurrentColumn"/>
    </MenuFlyout>
</ResourceDictionary>
